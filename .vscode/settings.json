{
    "extensions.ignoreRecommendations": true,
    "C_Cpp.default.configurationProvider": "particle.particle-vscode-core",
    "cortex-debug.openocdPath": "${command:particle.getDebuggerOpenocdPath}",
    "files.associations": {
        "*.ino": "cpp",
        "memory": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "xutility": "cpp",
        "xmemory": "cpp",
        "atomic": "cpp",
        "chrono": "cpp",
        "xstring": "cpp"
    },
    "particle.targetDevice": "Boron_1",
    "particle.targetPlatform": "boron",
    "particle.compileDefines": [
        "-DDebug",
        "-DBLE_WIRING_DEBUG_ENABLED",
        "-DDEBUG_BUILD"
    ],
    "particle.firmwareVersion": "2.0.0"
}